ControlMaster auto
    ControlPath ~/tmp/ssh_mux_%h_%p_%r
ControlPersist 4h

Host i.me
    User root
    Port 33
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    IdentityFile ~/.ssh/id_rsa
    DynamicForward 8888

Host 10.*.*.*
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host 10.10.*.*
    ProxyCommand /bin/nc -X 5 -x 127.0.0.1:8888 %h %p

Host *
    User root
    Port 22
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
